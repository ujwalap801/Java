
//Types of  arguments
 class xyz{
    void show(String a){
        System.out.println("1");
    }
 }
public class Test extends xyz {
    void show(String b)
    {
        System.out.println("2");
    }
    public static void main(String[] args) {
        Test t = new Test();
        t.show("uju");

        xyz s = new xyz();
        s.show("vinay");
    }
}


