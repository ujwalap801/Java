// No of arguments
public class Number {

    void show(int a, int b)
    {
        System.out.println("1");
    }


    void show(int a)
    {
        System.out.println("2");
    }


    public static void main(String[] args) {
        Number t = new Number();
        t.show(10,20);
        t.show(10);
    }


    
