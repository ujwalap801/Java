

class Employee {

    private int emp_id;

    public void setEmpid(int emp_id)
    {
         this.emp_id = emp_id;
    }

    public int getEmpid()
    {
        return emp_id;
    }
}

public class Company {
public static void main(String[] args) {
    Employee e = new Employee();
    e.setEmpid(101);
    System.out.println(e.getEmpid());

}
    
}
